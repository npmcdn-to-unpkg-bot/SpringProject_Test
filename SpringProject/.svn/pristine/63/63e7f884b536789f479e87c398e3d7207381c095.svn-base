package board.bean;

public class RecommandDTO {
	private int seq;
	private String  recommand_name;
	private String recommand_price;
	private String recommand_image;
	
	public int getSeq() {
		return seq;
	}
	public void setSeq(int seq) {
		this.seq = seq;
	}
	public String getRecommand_name() {
		return recommand_name;
	}
	public void setRecommand_name(String recommand_name) {
		this.recommand_name = recommand_name;
	}
	public String getRecommand_price() {
		return recommand_price;
	}
	public void setRecommand_price(String recommand_price) {
		this.recommand_price = recommand_price;
	}
	public String getRecommand_image() {
		return recommand_image;
	}
	public void setRecommand_image(String recommand_image) {
		this.recommand_image = recommand_image;
	}
	
}
